{
    "name": "curlyspoon/cms",
    "description": "",
    "keywords": ["curlyspoon", "cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "dev.gummibeer@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "curlyspoon/core": "^1.3",
        "curlyspoon/nested-options-resolver": "^1.0",
        "illuminate/support": "5.6.*",
        "illuminate/view": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Curlyspoon\\Cms\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Curlyspoon\\Cms\\CurlyspoonServiceProvider"
            ],
            "aliases": {
                "ElementManager": "Curlyspoon\\Cms\\Facades\\ElementManager"
            }
        }
    }
}
